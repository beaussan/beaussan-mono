{
  "name": "new-tab-page",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/new-tab-page",
  "projectType": "application",
  "implicitDependencies": ["new-tab-page-hasura"],
  "targets": {
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn graphql-codegen --config apps/new-tab-page/gqlCodegen.ts"
      }
    },
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/new-tab-page",
        "outputPath": "dist/apps/new-tab-page"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/new-tab-page"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "new-tab-page:build",
        "dev": true,
        "hostname": "0.0.0.0",
        "port": 4238
      },
      "configurations": {
        "development": {
          "buildTarget": "new-tab-page:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "new-tab-page:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "new-tab-page:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/new-tab-page/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/new-tab-page/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": ["scope:dash"]
}

{
  "name": "dash-data-user",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dash/data/user/src",
  "projectType": "library",
  "tags": ["scope:dash", "type:data"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/dash/data/user/**/*.{ts,tsx,js,jsx}",
          "libs/dash/data/user/**/*.json"
        ]
      }
    },
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/dash/data/user"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/dash/data/user"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../coverage/libs/dash/data/user"
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn graphql-codegen --config libs/dash/data/user/gqlCodegen.ts"
      },
      "configurations": {
        "default": {},
        "watch": {
          "command": "yarn graphql-codegen --config libs/dash/data/user/gqlCodegen.ts --watch"
        }
      }
    }
  }
}

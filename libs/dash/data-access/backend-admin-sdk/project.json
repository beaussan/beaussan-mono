{
  "name": "dash-data-access-backend-admin-sdk",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dash/data-access/backend-admin-sdk/src",
  "projectType": "library",
  "targets": {
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn graphql-codegen --config libs/dash/data-access/backend-admin-sdk/gqlCodegen.ts"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/dash/data-access/backend-admin-sdk",
        "main": "libs/dash/data-access/backend-admin-sdk/src/index.ts",
        "tsConfig": "libs/dash/data-access/backend-admin-sdk/tsconfig.lib.json",
        "assets": ["libs/dash/data-access/backend-admin-sdk/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/dash/data-access/backend-admin-sdk/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/dash/data-access/backend-admin-sdk/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:dash", "type:data-access"]
}

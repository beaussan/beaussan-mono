{
  "name": "shared-data-next-auth-urql-hasura",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data/next-auth-urql-hasura/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:data"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/data/next-auth-urql-hasura/**/*.{ts,tsx,js,jsx}",
          "libs/shared/data/next-auth-urql-hasura/**/*.json"
        ]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/shared/data/next-auth-urql-hasura"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/libs/shared/data/next-auth-urql-hasura"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../coverage/libs/shared/data/next-auth-urql-hasura"
      }
    }
  }
}

{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@beaussan/internal-generator": [
        "libs/nx-plugins/internal-generator/src/index.ts"
      ],
      "@beaussan/nx-hasura": ["libs/nx-plugins/nx-hasura/src/index.ts"],
      "@beaussan/shared/utils/supertokens/node": [
        "libs/shared/utils/supertokens/node/src/index.ts"
      ],
      "@beaussan/shared/utils/supertokens/react": [
        "libs/shared/utils/supertokens/react/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
